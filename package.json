{
  "name": "pycalc",
  "displayName": "pycalc",
  "publisher": "pycalc",
  "description": "The pycalc plugin enables the execution of python code directly within the editor upon pressing the Enter key.",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pycalc-plugin/vscode.git"
  },
  "homepage": "https://github.com/pycalc-plugin/vscode/blob/master/README.md",
  "license": "Apache-2.0",
  "categories": [
    "Programming Languages",
    "Notebooks",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "when": "pycalc.enabled",
          "command": "pycalc.enable",
          "group": "z_commands"
        },
        {
          "when": "!pycalc.enabled",
          "command": "pycalc.disable",
          "group": "z_commands"
        },
        {
          "when": "editorHasSelection||editorFocus",
          "command": "pycalc.selected",
          "group": "z_commands"
        }
      ],
      "commandPalette": [
        {
          "command": "pycalc.enable",
          "when": "false"
        },
        {
          "command": "pycalc.disable",
          "when": "false"
        },
        {
          "command": "pycalc.enter",
          "when": "false"
        },
        {
          "command": "pycalc.selected",
          "when": "false"
        }
      ]
    },
    "commands": [
      {
        "command": "pycalc.enable",
        "title": "pycalc [✓]",
        "when": "false"
      },
      {
        "command": "pycalc.disable",
        "title": "pycalc [×]",
        "when": "false"
      },
      {
        "command": "pycalc.enter",
        "title": "pycalc"
      },
      {
        "command": "pycalc.selected",
        "title": "pycalc selected"
      }
    ],
    "keybindings": [
      {
        "command": "pycalc.enter",
        "key": "enter",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0"
  },
  "dependencies": {
    "jsdom": "^25.0.1"
  }
}
